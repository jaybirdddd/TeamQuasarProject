};

// ========= Oxygen Leak core =========
(function(){
  const V = () => SugarCube.State.variables;

  window.OXY_initScenario = function() {
    const v = V();
    v.oxy_pressure = Math.floor(Math.random()*21) + 70;
    const pipes = ['Green','Blue','Red','Yellow'];
    v.oxy_leak = pipes[Math.floor(Math.random()*pipes.length)];
    v.oxy_valves = { Red:true, Blue:true, Yellow:true, Green:true };
    v.oxy_sealed = false; v.oxy_failed = false; v.oxy_okTicks = 0;
    OXY_stopLoop(); OXY_startLoop();
    setTimeout(OXY_render, 0);
  };

  window.OXY_expected = function() {
    const v = V();
    const exp = { Red:true, Blue:true, Yellow:true, Green:true };
    switch (v.oxy_leak) {
      case 'Green':  exp.Red=false;  exp.Blue=true;  break;
      case 'Blue':   exp.Yellow=true;exp.Green=false;break;
      case 'Red':    exp.Blue=false; exp.Yellow=true;break;
      case 'Yellow': exp.Red=true;   exp.Green=false;break;
    }
    if (v.oxy_pressure < 80) exp.Yellow = false;
    if (v.oxy_pressure < 50) { exp.Red=false; exp.Blue=false; exp.Yellow=false; exp.Green=false; }
    return exp;
  };

  window.OXY_isCorrect = function() {
    const v = V(), exp = OXY_expected(), valves = v.oxy_valves;
    for (const k in exp) if (valves[k] !== exp[k]) return false;
    return true;
  };

  window.OXY_tick = function() {
    const v = V();
    if (v.oxy_failed || v.oxy_sealed) return;
    const correct = OXY_isCorrect();

    if (correct) {
      v.oxy_pressure = Math.min(100, v.oxy_pressure + 1);
      v.oxy_okTicks = (v.oxy_okTicks||0) + 1;
    } else {
      const exp = OXY_expected();
      let bad = 0;
      for (const k in exp) if (v.oxy_valves[k] !== exp[k]) bad++;
      const drop = 1 + bad;
      v.oxy_pressure = Math.max(0, v.oxy_pressure - drop);
      v.oxy_okTicks = 0;
    }

    if (v.oxy_pressure <= 0) { v.oxy_failed = true; OXY_stopLoop(); }
    if (v.oxy_okTicks >= 3) { v.oxy_sealed = true; OXY_stopLoop(); }
    OXY_render();
  };

  window.OXY_startLoop = function(){
    OXY_stopLoop();
    window._oxyTimer = setInterval(OXY_tick, 1000);
    OXY_tick();
  };

  window.OXY_stopLoop = function(){
    if (window._oxyTimer) { clearInterval(window._oxyTimer); window._oxyTimer = null; }
  };

  window.OXY_toggleValve = function(color){
    const v = V();
    v.oxy_valves[color] = !v.oxy_valves[color];
    OXY_render();
  };

  window.OXY_render = function(){
    const v = V();
    const num = document.getElementById('oxy-pressure-num');
    const bar = document.getElementById('oxy-gauge-bar');
    if (num) num.textContent = (v.oxy_pressure|0) + '%';
    if (bar) bar.style.width = Math.max(0, Math.min(100, v.oxy_pressure)) + '%';

    ['Red','Blue','Yellow','Green'].forEach(c=>{
      const b = document.getElementById('v-'+c);
      if (b) {
        const open = v.oxy_valves[c];
        b.textContent = c + ': ' + (open ? 'OPEN' : 'CLOSED');
        b.className = 'valve ' + (open ? 'open' : 'closed');
      }
    });

    const tip = document.getElementById('oxy-leak-tip');
    if (tip) tip.textContent = 'Bubbles observed near ' + v.oxy_leak + ' pipe.';
    const st = document.getElementById('oxy-status');
    if (st) {
      if (v.oxy_failed) st.innerHTML = '<span class="bad"> Oxygen depleted. Mission failed.</span>';
      else if (v.oxy_sealed) st.innerHTML = '<span class="good"> Leak sealed! Pressure stabilizing.</span>';
      else st.textContent = 'Adjust valves to stop the leak.';
    }
  };
})();

