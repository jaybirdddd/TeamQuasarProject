window.playBGM = function (id, src) {
  try {
    var abs = new URL(src, location.href).href;
    if (window._bgm.src !== abs) {
      window._bgm.pause();
      window._bgm.src = abs;
      window._bgm.load();
    }
    window._bgm.play().catch(function(e){
      console.warn('Autoplay blocked, click once to enable sound.', e);
    });
    window.currentBGMId = id;
  } catch (e) {
